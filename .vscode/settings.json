{
    "files.exclude": {
        "**/.build": true,
        "**/DerivedData": true,
        "**/*.xcodeproj/xcuserdata": true,
        "**/*.xcworkspace/xcuserdata": true,
        "**/build": true,
        "**/node_modules": true
    },
    "search.exclude": {
        "**/.build": true,
        "**/DerivedData": true,
        "**/*.xcodeproj/xcuserdata": true,
        "**/*.xcworkspace/xcuserdata": true,
        "**/build": true
    },
    "swift.buildPath": ".build",
    "swift.targetBuildPath": ".build/debug",
    "swift.debuggerPath": "/usr/bin/lldb",
    "swift.swiftPath": "/Applications/Xcode.app/Contents/Developer/Toolchains/XcodeDefault.xctoolchain/usr/bin/swift",
    "swift.swiftPackageManagerPath": "/Applications/Xcode.app/Contents/Developer/Toolchains/XcodeDefault.xctoolchain/usr/bin/swift-package",
    "swift.swiftc": "/Applications/Xcode.app/Contents/Developer/Toolchains/XcodeDefault.xctoolchain/usr/bin/swiftc",
    "swift.xcodePath": "/Applications/Xcode.app/Contents/Developer",
    "swift.toolchainPath": "/Applications/Xcode.app/Contents/Developer/Toolchains/XcodeDefault.xctoolchain",
    "swift.buildConfiguration": "debug",
    "swift.buildArguments": [
        "--configuration",
        "debug"
    ],
    "swift.disableSwiftSyntaxHighlighter": false,
    "swift.disableSwiftLint": false,
    "swift.enableCodeCompletion": true,
    "swift.enableHover": true,
    "swift.enableSignatureHelp": true,
    "swift.enableDiagnostics": true,
    "sourcekit-lsp.serverPath": "/Applications/Xcode.app/Contents/Developer/Toolchains/XcodeDefault.xctoolchain/usr/bin/sourcekit-lsp",
