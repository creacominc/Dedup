    "swift.sourcekit-lsp.serverArguments": [],
    "swift.sourcekit-lsp.toolchainPath": "/Applications/Xcode.app/Contents/Developer/Toolchains/XcodeDefault.xctoolchain",
    "terminal.integrated.env.osx": {
        "DEVELOPER_DIR": "/Applications/Xcode.app/Contents/Developer",
        "PATH": "/usr/bin:/bin:/usr/sbin:/sbin:/Applications/Xcode.app/Contents/Developer/Toolchains/XcodeDefault.xctoolchain/usr/bin:/Applications/Xcode.app/Contents/Developer/usr/bin"
    },
    "remote.containers.defaultExtensions": [],
    "remote.containers.showStartNotification": false,
    "remote.containers.ignoreSubmodules": true
